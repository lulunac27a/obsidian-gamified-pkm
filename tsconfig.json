{
  "compilerOptions": {
	"baseUrl": "./src",
	"inlineSourceMap": false,
    "inlineSources": true,
	"sourceMap": true,
    "module": "ESNext",
    "allowJs": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "isolatedModules": true,
    "jsx": "react",
	"strictNullChecks": true,
    "types": ["jest", "obsidian"],
	"allowSyntheticDefaultImports": true,
    "target": "ES2021",
	"lib": [
      "DOM",
      "ES5",
      "ES6",
      "ES7",
	  "ES2021"
    ]
  },
  "include": [
	  "node_modules/@types",
	  "**/*.ts"
, "jest.config.test"  ]
}
